  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0"
